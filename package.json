{
  "name": "fox",
  "version": "1.10.0",
  "description": "ClickFox Fox Application",
  "repository": {
    "url": "https://git.clickfox.net/ui/ui"
  },
  "author": "Fox UI Team",
  "license": "Commercial",
  "scripts": {
    "build.dev": "ng build --configuration=dev",
    "build.prod": "ng build --prod --aot --base-href='/fox/' --deploy-url='/fox/'",
    "build.big.prod": "NODE_OPTIONS=--max_old_space_size=4096 ng build --prod --aot --base-href='/fox/' --deploy-url='/fox/'",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "clean.install": "rm -rf node_modules package-lock.json && npm install",
    "copy": "cp app/{index.html,styles.css, tools/config.js} built/",
    "copytemplates": "cp app/{*.html,*.css} built/app/",
    "docs": "compodoc -s -p tsconfig.app.json",
    "docs.local": "./node_modules/.bin/compodoc -s -p ./tsconfig.app.json",
    "cypress": "./node_modules/.bin/cypress open --env configFile=local",
    "cypress-run": "./node_modules/.bin/cypress run --env configFile=local",
    "cypress-local-docker": "./node_modules/.bin/cypress open --env configFile=local-docker",
    "cypress-headless": "./node_modules/.bin/cypress run --env configFile=docker",
    "cypress-chrome": "./node_modules/.bin/cypress run --browser chrome --env configFile=dev",
    "cypress-local": "./node_modules/.bin/cypress open --env configFile=test",
    "cypress-local-chrome": "./node_modules/.bin/cypress run --browser chrome --env configFile=test",
    "cypress-chrome-docker": "./node_modules/.bin/cypress run --browser chrome --env configFile=local-docker",
    "cypress-docker": "./node_modules/.bin/cypress run --browser chrome --env configFile=docker",
    "cypress-all": "node cypress.tester.js",
    "docker-test": "docker-compose -f docker-compose.local-build.yml up --build --abort-on-container-exit",
    "hmr": "ng serve --hmr -e=hmr",
    "karma": "karma",
    "karma.start": "karma start",
    "lint": "ng lint",
    "ng": "ng",
    "ng.build": "ng build",
    "reinstall": "rimraf node_modules && npm cache clean && npm install",
    "start": "NODE_OPTIONS=--max_old_space_size=4096 ng serve --configuration=$ENV --aot --hmr",
    "test": "ng test --watch=false --sourceMap=false --codeCoverage=false",
    "test:head": "ng test --browsers=Chrome",
    "tsd": "tsd",
    "preinstall": "cp git/hooks/* .git/hooks"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "7.2.15",
    "@angularclass/hmr": "^2.1.3",
    "@compodoc/compodoc": "^1.0.9",
    "@types/async": "^2.0.47",
    "@types/d3": "4.10.0",
    "@types/d3-scale": "^1.0.11",
    "@types/d3-time-format": "2.0.4",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.5.9",
    "async": "^2.0.0",
    "chalk": "^1.1.3",
    "check-dependencies": "^1.1.0",
    "codelyzer": "^5.0.0",
    "cypress": "^3.1.5",
    "cypress-plugin-retries": "^1.2.0",
    "cypress-multi-reporters": "^1.1.9",
    "del": "^1.1.1",
    "express": "~4.14.0",
    "jasmine": "^2.9.0",
    "jasmine-core": "^2.9.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^0.5.5",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-remap-istanbul": "^0.6.0",
    "karma-spec-reporter": "0.0.32",
    "lite-server": "^2.3.0",
    "mocha": "^6.1.4",
    "mocha-junit-reporters": "^1.23.0",
    "opn": "^4.0.2",
    "run-sequence": "^1.2.2",
    "semver": "^5.5.0",
    "serve-static": "^1.11.1",
    "slash": "~1.0.0",
    "tiny-lr": "^0.2.1",
    "traceur": "^0.0.91",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1",
    "typescript": "3.2.4",
    "typescript-register": "^1.1.0",
    "typings": "^1.4.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "yargs": "^4.8.0"
  },
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "6.4.2",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@angular/material": "6.4.2",
    "@angular/material-moment-adapter": "6.4.2",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ngrx/effects": "^6.0.0",
    "@ngrx/store": "6.1.0",
    "@ngrx/store-devtools": "^6.0.0",
    "ag-grid": "^18.1.1",
    "ag-grid-angular": "^18.1.0",
    "chart.js": "^2.6.0",
    "core-js": "^2.4.1",
    "d3": "^4.12.2",
    "dragula": "^3.7.1",
    "equals": "^1.1.0",
    "es6-module-loader": "^0.17.8",
    "es6-promise": "^4.2.4",
    "es6-shim": "^0.35.3",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "lokijs": "^1.5.5",
    "moment": "2.22.2",
    "ng2-charts": "^1.6.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.2.2",
    "tslib": "^1.9.0",
    "zone.js": "0.8.29"
  }
}
